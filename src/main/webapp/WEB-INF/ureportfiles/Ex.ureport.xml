	left join biscompany b 
		on a.biscompyId =b.ID
	where  DATE_FORMAT(a.date,'%Y%m%d') >= DATE_FORMAT(:bdate,'%Y%m%d') 
	and DATE_FORMAT(a.date,'%Y%m%d') <= DATE_FORMAT(:edate,'%Y%m%d')
        and a.deptid=:deptid
]]></sql><field name="title"/><field name="total_price"/><field name="name"/><field name="ID"/><field name="deptId"/><field name="Date"/><field name="Cod"/><field name="Ph"/><field name="Ss"/><field name="Nh"/><field name="Tn"/><field name="Tp"/><field name="Bod"/><field name="biscompyId"/><field name="intflux"/><field name="influxtotal"/><field name="temperature"/><field name="salinity"/><field name="keyinId"/><field name="Cod_price"/><field name="Nh_price"/><field name="Ph_price"/><field name="Tn_price"/><field name="Ss_price"/><field name="Tp_price"/><field name="valid"/><parameter name="deptid" type="String" default-value="00101"/><parameter name="bdate" type="Date" default-value="2017-06-17"/><parameter name="edate" type="Date" default-value="2017-07-17"/></dataset><dataset name="外单位水样表月份" type="sql"><sql><![CDATA[SELECT date_format(date,'%Y年%m月')  from analysis_ext where  DATE_FORMAT(date,'%Y%m') = DATE_FORMAT(:date,'%Y%m') group by date_format(date,'%Y%m')]]></sql><field name="date_format(date,'%Y年%m月')"/><parameter name="date" type="Date" default-value="2017-01-06"/></dataset><dataset name="XX年XX月XX公司" type="sql"><sql><![CDATA[
