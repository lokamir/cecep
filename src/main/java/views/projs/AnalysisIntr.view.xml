<?xml version="1.0" encoding="UTF-8"?>
<ViewConfig>
  <Arguments/>
  <Context/>
  <Model>
    <DataType name="dataTypeAnalysisIntr" parent="global:dataTypeAnalysisIntr">
      <PropertyDef name="process">
        <Property name="label">工艺段</Property>
        <Property name="dataType">dataTypeProcess</Property>
      </PropertyDef>
      <PropertyDef name="deptid">
        <Property name="label">水厂名称</Property>
        <Property name="defaultValue">00101</Property>
        <Property name="readOnly">true</Property>
      </PropertyDef>
    </DataType>
    <DataType name="dataTypeProcess" parent="global:dataTypeProcess">
      <Property name="defaultDisplayProperty">name</Property>
      <PropertyDef name="name">
        <Property></Property>
        <Property name="label">工艺段名称</Property>
      </PropertyDef>
    </DataType>
    <DataType name="dataTypeformCondition">
      <PropertyDef name="beginDate">
        <Property name="label">开始日期</Property>
        <Property name="displayFormat">Y年m月d日</Property>
        <Property name="dataType">Date</Property>
      </PropertyDef>
      <PropertyDef name="endDate">
        <Property name="label">结束日期</Property>
        <Property name="displayFormat">Y年m月d日</Property>
        <Property name="dataType">Date</Property>
      </PropertyDef>
      <PropertyDef name="deptid">
        <Property name="label">水厂名称</Property>
        <Property name="mapping">
          <Property name="mapValues">${dorado.getDataProvider(&quot;el#mapBdf2Depts&quot;).getResult()}</Property>
        </Property>
        <Property name="defaultValue">00101</Property>
        <Property name="readOnly">true</Property>
      </PropertyDef>
      <PropertyDef name="specimen">
        <Property name="dataType">String</Property>
        <Property name="label">样品名称</Property>
      </PropertyDef>
      <PropertyDef name="biscompany">
        <Property name="label">接管企业</Property>
        <Property name="dataType">dataTypeBiscompany</Property>
      </PropertyDef>
      <PropertyDef name="process">
        <Property name="label">工艺段</Property>
        <Property name="dataType">dataTypeProcess</Property>
      </PropertyDef>
    </DataType>
  </Model>
  <View>
    <Property name="title">内场数据维护</Property>
    <DataSet id="dataSetAnalysisIntr">
      <Property name="dataProvider">analysisIntrDao#queryPage</Property>
      <Property name="pageSize">20</Property>
      <Property name="dataType">[dataTypeAnalysisIntr]</Property>
    </DataSet>
    <UpdateAction id="actionUpdate">
      <Property name="successMessage">保存成功!</Property>
      <Property name="executingMessage">正在保存...</Property>
      <Property name="dataResolver">analysisIntrDao#saveAnalysisIns</Property>
      <UpdateItem>
        <Property name="dataSet">dataSetAnalysisIntr</Property>
      </UpdateItem>
    </UpdateAction>
    <ToolBar>
      <DataPilot>
        <ClientEvent name="onSubControlAction">var datas = view.get(&quot;#dataSetAnalysisIntr.data&quot;);&#xD;
var action = view.get(&quot;#actionUpdate&quot;);&#xD;
switch(arg.code){&#xD;
	case &quot;+&quot;:{&#xD;
		datas.insert();&#xD;
		view.get(&quot;#dialogAnalysisIntr&quot;).show();&#xD;
		arg.processDefault=false;&#xD;
		break;&#xD;
	}&#xD;
	case &quot;-&quot;:{&#xD;
		dorado.MessageBox.confirm(&quot;您真的想删除当前数据吗?&quot;,function(){&#xD;
			datas.remove();&#xD;
			action.execute();&#xD;
		});&#xD;
		arg.processDefault=false;&#xD;
		break;&#xD;
	}&#xD;
}</ClientEvent>
        <Property name="dataSet">dataSetAnalysisIntr</Property>
        <Property name="itemCodes">pages,+,-,x</Property>
      </DataPilot>
      <Separator/>
      <ToolBarButton>
        <ClientEvent name="onClick">var data=view.get(&quot;#dataSetAnalysisIntr.data&quot;);&#xD;
if(data.current){&#xD;
	view.get(&quot;#dialogAnalysisIntr&quot;).show();&#xD;
}else{&#xD;
	dorado.MessageBox.alert(&quot;当前表没有信息可供编辑!&quot;);&#xD;
}&#xD;
</ClientEvent>
        <Property name="caption">修改</Property>
        <Property name="icon">url(>skin>common/icons.gif) -200px -0px</Property>
      </ToolBarButton>
      <ToolBarButton>
        <Property name="caption">保存</Property>
        <Property name="icon">url(>skin>common/icons.gif) -140px -20px</Property>
        <Property name="action">actionUpdate</Property>
        <Property name="visible">false</Property>
        <Property name="hideMode">display</Property>
      </ToolBarButton>
      <ToolBarButton>
        <ClientEvent name="onClick">var data = view.get(&quot;#formCondition.entity&quot;);&#xD;
with (view.get(&quot;#dataSetAnalysisIntr&quot;)){&#xD;
	set(&quot;parameter&quot;, data);&#xD;
	flushAsync();&#xD;
}</ClientEvent>
        <Property name="caption">查询</Property>
        <Property name="icon">url(>skin>common/icons.gif) -20px -40px</Property>
      </ToolBarButton>
    </ToolBar>
    <AutoForm id="formCondition">
      <Property name="cols">*,*,*</Property>
      <Property name="createPrivateDataSet">true</Property>
      <Property name="dataType">dataTypeformCondition</Property>
      <AutoFormElement>
        <Property name="name">beginDate</Property>
        <Property name="label">开始时间</Property>
        <Property name="property">beginDate</Property>
        <Property name="trigger">defaultDateDropDown</Property>
        <Editor/>
      </AutoFormElement>
      <AutoFormElement>
        <Property name="name">endDate</Property>
        <Property name="label">结束时间</Property>
        <Property name="property">endDate</Property>
        <Property name="trigger">defaultDateDropDown</Property>
        <Editor/>
      </AutoFormElement>
      <AutoFormElement>
        <Property name="name">process</Property>
        <Property name="label">工艺段</Property>
        <Property name="property">process</Property>
        <Property name="trigger">dataSetDropDownProcess</Property>
        <Editor/>
      </AutoFormElement>
      <AutoFormElement>
        <Property name="name">deptid</Property>
        <Property name="label">水厂名称</Property>
        <Property name="property">deptid</Property>
        <Editor/>
      </AutoFormElement>
    </AutoForm>
    <DataGrid>
      <Property name="dataSet">dataSetAnalysisIntr</Property>
      <Property name="dataType">dataTypeAnalysisIntr</Property>
      <Property name="readOnly">true</Property>
      <Property name="showFooter">true</Property>
      <DataColumn name="date">
        <Property name="property">date</Property>
      </DataColumn>
      <DataColumn name="process">
        <Property name="property">process</Property>
        <Property name="trigger">dataSetDropDownProcess</Property>
      </DataColumn>
      <DataColumn name="cod">
        <Property name="property">cod</Property>
      </DataColumn>
      <DataColumn name="ph">
        <Property name="property">ph</Property>
      </DataColumn>
      <DataColumn name="ss">
        <Property name="property">ss</Property>
      </DataColumn>
      <DataColumn name="nh">
        <Property name="property">nh</Property>
      </DataColumn>
      <DataColumn name="tn">
        <Property name="property">tn</Property>
      </DataColumn>
      <DataColumn name="tp">
        <Property name="property">tp</Property>
      </DataColumn>
      <DataColumn name="bod">
        <Property name="property">bod</Property>
      </DataColumn>
      <DataColumn name="temperature">
        <Property name="property">temperature</Property>
      </DataColumn>
      <DataColumn name="salinity">
        <Property name="property">salinity</Property>
      </DataColumn>
      <DataColumn name="deptid">
        <Property name="property">deptid</Property>
      </DataColumn>
    </DataGrid>
    <Dialog id="dialogAnalysisIntr">
      <Property name="center">true</Property>
      <Property name="width">1000</Property>
      <Property name="height">400</Property>
      <Property name="modal">true</Property>
      <Property name="closeable">false</Property>
      <Property name="caption">数据编辑</Property>
      <Buttons>
        <Button>
          <ClientEvent name="onClick">var dialog=view.get(&quot;#dialogAnalysisIntr&quot;);&#xD;
var action=view.get(&quot;#actionUpdate&quot;);&#xD;
dorado.MessageBox.confirm(&quot;您真的要保存吗?&quot;,function(){&#xD;
	action.execute(function(){&#xD;
		dialog.hide();&#xD;
	});&#xD;
})</ClientEvent>
          <Property name="caption">确定</Property>
          <Property name="icon">url(>skin>common/icons.gif) -140px -20px</Property>
        </Button>
        <Button>
          <ClientEvent name="onClick">var data=view.get(&quot;#dataSetAnalysisIntr&quot;).getData();&#xD;
var dialog=view.get(&quot;#dialogAnalysisIntr&quot;);&#xD;
dorado.MessageBox.confirm(&quot;您真的要取消当前操作吗？&quot;,function(){&#xD;
	data.cancel();&#xD;
	dialog.hide();&#xD;
});</ClientEvent>
          <Property name="caption">取消</Property>
          <Property name="icon">url(>skin>common/icons.gif) -40px -0px</Property>
        </Button>
      </Buttons>
      <Children>
        <AutoForm>
          <Property name="dataSet">dataSetAnalysisIntr</Property>
          <Property name="cols">*,*,*</Property>
          <AutoFormElement name="date">
            <Property name="name">date</Property>
